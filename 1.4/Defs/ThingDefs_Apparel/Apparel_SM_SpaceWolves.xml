  <ThingDef ParentName="SMArmorBase">
    <!-- space wolves Grunt Armour -->
    <defName>SpaceWolvesArmor</defName>
    <label>space wolves armor</label>
    <graphicData>
      <texPath>Things/SpaceMarines/SpaceWolves/Armor/SpaceWolf_Normal</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <apparel>
      <wornGraphicPath>Things/SpaceMarines/SpaceWolves/Armor/SpaceWolf_Normal</wornGraphicPath>
      <tags Inherit="false">
        <li>AstartesApparel</li>
        <li>SpaceWolvesApparel</li>
